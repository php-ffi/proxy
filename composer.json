{
    "name": "ffi/proxy",
    "type": "library",
    "description": "PHP FFI Proxy",
    "license": "MIT",
    "keywords": ["ffi", "proxy"],
    "support": {
        "source": "https://github.com/php-ffi/proxy",
        "issues": "https://github.com/php-ffi/proxy/issues",
        "docs": "https://github.com/php-ffi/proxy/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Nesmeyanov Kirill",
            "email": "nesk@xakep.ru",
            "homepage": "https://serafimarts.ru",
            "role": "maintainer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-ffi": "*",
        "symfony/polyfill-php80": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "FFI\\Proxy\\": "src"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^5.4",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload-dev": {
        "psr-4": {
            "FFI\\Proxy\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev",
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
